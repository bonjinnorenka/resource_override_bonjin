{
  "manifest_version": 3,
  "name": "URL Rewrite Bonjin",
  "version": "1.0.0",
  "description": "正規表現でURLをマッチし、別のURLに書き換える拡張機能",
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js",
    "scripts": [
      "background.js"
    ]
  },
  "devtools_page": "devtools.html",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "url-rewrite@example.com",
      "strict_min_version": "109.0"
    }
  },
  "data_collection_permissions": {
    "data_collected": false,
    "disclosure": "This extension does not collect any user data.",
    "data_sold": false
  }
}